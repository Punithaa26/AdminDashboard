{
  "name": "realtime-admin-dashboard-backend",
  "version": "1.0.0",
  "description": "Real-time admin dashboard backend with WebSocket support",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js",
    "seed": "node scripts/seedingData.js",
    "seed-analytics": "node scripts/seedAnalyticsData.js",
    "seed-users": "node scripts/seedUsersData.js",
    "seed-content": "node scripts/contentSeeder.js",
    "seed:content": "node -e \"require('./scripts/contentSeeder').seedContent().then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "clear:content": "node -e \"require('./scripts/contentSeeder').clearContent().then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "update:content": "node -e \"require('./scripts/contentSeeder').updateContentSchema().then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "seed:all": "npm run seed:users && npm run seed:content",
    "reset:db": "npm run clear:content && npm run seed:content"
  },
  "keywords": [
    "admin",
    "dashboard",
    "realtime",
    "websocket",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-slow-down": "^1.6.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "uuid": "^11.1.0",
    "validator": "^13.11.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "your-repository-url"
  },
  "bugs": {
    "url": "your-repository-url/issues"
  },
  "homepage": "your-repository-url#readme"
}
